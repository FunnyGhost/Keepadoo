{
  "name": "keepadoo",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "start:ci": "http-server ./dist/keepadoo -a localhost -p 4200 -c-1",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "jest",
    "test:coverage": "jest --runInBand --coverage",
    "test:snapshot": "jest --coverage -u",
    "test:watch": "jest --clearCache && jest --watch",
    "e2e": "cypress open",
    "cy:run": "$(npm bin)/cypress run --record --key a381f13e-bf4e-4506-8af0-5dd128b93248",
    "e2e:ci": "start-server-and-test start:ci  http://localhost:4200 cy:run",
    "lint": "ng lint",
    "build-docker-image": "docker build -t keepadoo .",
    "docker-run": "docker run -d -p 8080:80 keepadoo",
    "docker-build": "docker image build -t utukku/keepadoo:1.0 -t registry.heroku.com/keepadoo/web .",
    "docker-push": "docker push utukku/keepadoo:1.0 ",
    "login-heroku": "heroku container:login",
    "push-heroku": "heroku container:push web --app keepadoo",
    "release-heroku": "heroku container:release web --app keepadoo"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.0",
    "@angular/cdk": "7.1.0",
    "@angular/common": "^7.1.0",
    "@angular/compiler": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@angular/fire": "5.1.1",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "^7.1.0",
    "@angular/http": "^7.1.0",
    "@angular/material": "7.1.0",
    "@angular/platform-browser": "^7.1.0",
    "@angular/platform-browser-dynamic": "^7.1.0",
    "@angular/router": "^7.1.0",
    "@briebug/jest": "^1.3.0",
    "@ngrx/effects": "6.1.2",
    "@ngrx/store": "6.1.2",
    "@ngrx/store-devtools": "6.1.2",
    "core-js": "^2.5.6",
    "firebase": "5.6.0",
    "rollbar": "2.5.1",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/language-service": "^7.1.0",
    "@types/chance": "1.0.1",
    "@types/jest": "^23.3.1",
    "@types/node": "~10.1.2",
    "chance": "1.0.18",
    "codelyzer": "~4.3.0",
    "cypress": "3.1.3",
    "http-server": "0.11.1",
    "jest": "23.2.0",
    "jest-marbles": "2.1.0",
    "jest-preset-angular": "5.2.3",
    "jest-sonar-reporter": "^2.0.0",
    "protractor": "~5.3.2",
    "start-server-and-test": "1.7.11",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "~3.1.6"
  }
}
