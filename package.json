{
  "name": "keepadoo",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "jest",
    "test:coverage": "jest --runInBand --coverage",
    "test:snapshot": "jest --coverage -u",
    "test:watch": "jest --clearCache && jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-docker-image": "docker build -t keepadoo .",
    "docker-run": "docker run -d -p 8080:80 keepadoo",
    "docker-build": "docker image build -t utukku/keepadoo:1.0 -t registry.heroku.com/keepadoo/web .",
    "docker-push": "docker push utukku/keepadoo:1.0 ",
    "docker-push-heroku": "docker push registry.heroku.com/keepadoo/web",
    "release-heroku": "heroku container:release web --app keepadoo",
    "docker-deploy": "docker run -d -p 80:80 --name keepadoo utukku/keepadoo:1.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.1",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "6.4.1",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@briebug/jest": "^1.3.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "angularfire2": "^5.0.0-rc.11",
    "core-js": "^2.5.6",
    "firebase": "^5.0.4",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular/cli": "~6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jest": "^23.3.1",
    "@types/node": "~10.1.2",
    "codelyzer": "~4.3.0",
    "istanbul-reports": "^1.3.0",
    "jest": "23.2.0",
    "jest-preset-angular": "5.2.3",
    "jest-sonar-reporter": "^2.0.0",
    "protractor": "~5.3.2",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "~2.9.2"
  }
}
